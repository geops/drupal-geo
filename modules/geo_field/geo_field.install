/**
 * Implementation of hook_install().
 */
function geo_field_install() {
  drupal_load('module', 'content');
  content_notify('install', 'geo');
}

/**
 * Implementation of hook_uninstall().
 */
function geo_field_uninstall() {
  drupal_load('module', 'content');
  content_notify('uninstall', 'geo');
}

